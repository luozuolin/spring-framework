<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean  id="myTestBean" name="name1,name2" class="org.springframework.luozlspringtest.MyTestBean"/>
    <bean id="beanNameAwareTest"  class="org.springframework.luozlspringtest.BeanNameAwareTest">
        <property name="beanName" value="beanname"/>
        <meta key="attr1" value="attr1value"/>
    </bean>
    <bean class="org.springframework.luozlspringtest.User" id="user">
        <property name="name" value="zhangsan"/>
        <property name="birthday" value="2011-1-1"/>
    </bean>
</beans>